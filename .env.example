# Core data pipeline
REDIS_URL=redis://localhost:6379/0

# Collector
COLLECTOR_STREAM_NAME=dfp:raw_ticks

# Stream buffer
BUFFER_PIPELINES=[{"name":"raw-backup","source_stream":"dfp:raw_ticks","targets":[{"name":"dfp:raw_ticks:backup","max_length":500000}],"trim_source_to":200000}]

# Data cleaner
CLEANER_INPUT_STREAM=dfp:raw_ticks
CLEANER_OUTPUT_STREAM=dfp:clean_ticks

# Feature pipeline windows示例
FEATURE_PIPELINE_WINDOWS=[{"name":"5s","window_size":5,"window_unit":"seconds"},{"name":"1m","window_size":1,"window_unit":"minutes"}]

# Strategy engine策略示例
STRATEGY_ENGINE_STRATEGIES=[{"name":"rapid-rise","module":"strategy_engine.strategies.rapid_rise","class_name":"RapidRiseStrategy","parameters":{"min_change":2.5,"min_volume":50000}}]

# Opportunity aggregator
OPPORTUNITY_AGG_OPPORTUNITY_STREAM=dfp:opportunities
OPPORTUNITY_AGG_OPPORTUNITY_CHANNEL=dfp:opportunities:ws

# Risk guard
RISK_GUARD_RISK_CHANNEL=dfp:risk_alerts

# Signal API
SIGNAL_API_MAX_RECORDS=200

# Backtest service
BACKTEST_DATA_LAKE_DIR=data-lake
BACKTEST_RESULTS_DIR=backtest-results

# Frontend (React) environment
REACT_APP_API_URL=http://localhost:9000
REACT_APP_PIPELINE_API_URL=http://localhost:8000
REACT_APP_PIPELINE_WS_URL=ws://localhost:8100/ws/opportunities
